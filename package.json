{
    "name": "es-esw-mq",
    "version": "1.5.0",
    "description": "ESW Module for Sample APP",
    "main": "/server/app.js",
    "engines": {
        "node": "11.13.0",
        "npm": "6.7.0"
    },
    "license": "ISC",
    "scripts": {
        "build": "npm-run-all clean clean:front lock webpack:front webpack:server webpack:node",
        "build:local": "npm-run-all clean clean:front -p gulp:default webpack:front-local webpack:server webpack:node-local",
        "clean": "rimraf server",
        "clean:front": "rimraf public/js ",
        "compodoc": "./node_modules/.bin/compodoc -p src/client/tsconfig.json",
        "docker:build": "docker build -t $npm_package_config_containername -f deploy/Dockerfile .",
        "docker:build-local": "docker build -t $npm_package_config_containername -f deploy/Dockerfile.local .",
        "docker:build-production": "docker build -t $npm_package_config_containernameprod -f deploy/Dockerfile .",
        "docker:push": "docker push $npm_package_config_containername",
        "docker:push-production": "docker push $npm_package_config_containernameprod",
        "gulp": "tsc --module commonjs --target ES6 --lib es6,dom gulpfile.ts",
        "gulp:default": "gulp default",
        "kube:dot": "node ../common/k8s/dot",
        "kube:init": "node ../common/k8s/init",
        "kube:init-prod": "node ../common/k8s/init-prod",
        "kube:local": "npm-run-all clean clean:front docker:build-local docker:push",
        "kube:local-prod": "npm-run-all build docker:build docker:push",
        "kube:production": "npm-run-all build docker:build-production docker:push-production",
        "lock": "npm install --package-lock",
        "start": "node --max-old-space-size=2048 ./server/app.js",
        "test": "jest --verbose --no-cache",
        "tslint": "npm-run-all tslint:s tslint:c",
        "tslint:c": "tslint -p ./src/client/tsconfig.json -c tslint.json './src/client/**/*.ts' --exclude './**/*.pug.tmpl.ts' --force",
        "tslint:s": "tslint -p ./src/server/tsconfig.json -c tslint.json './src/server/**/*.ts' --exclude './**/*.pug.tmpl.ts' --force",
        "webpack:front": "cross-env webpackenv=front webpack --mode production",
        "webpack:front-local": "cross-env webpackenv=front-local node --trace-deprecation ./node_modules/.bin/webpack -w --inline --progress --display-error-details --colors --mode development ",
        "webpack:node": "webpackenv=node webpack --mode production",
        "webpack:node-local": "cross-env webpackenv=node-local webpack -w --display-error-details --inline --progress --colors --mode development",
        "webpack:node-test": "cross-env webpackenv=node-test webpack -w --display-error-details --inline --progress --colors --mode development",
        "webpack:server": "cross-env webpackenv=server webpack --mode production"
    },
    "keywords": [
        "crr"
    ],
    "author": "johndoe@company.com",
    "repository": {
        "type": "git",
        "url": ""
    },
    "bugs": {
        "url": ""
    },
    "homepage": "",
    "dependencies": {
        "@cloudant/cloudant": "^4.0.0",
        "amqplib": "^0.5.3",
        "core-js": "^3.0.1",
        "getcfenv": "^1.0.3",
        "ibm-cos-sdk": "^1.4.5",
        "ibm_db": "^2.5.1",
        "lodash-es": "^4.17.11",
        "moment": "^2.24.0",
        "mqlight": "^2.0.2019041500",
        "print-hrtime": "^1.0.0",
        "pug": "^2.0.3",
        "request": "^2.88.0",
        "request-promise-native": "^1.0.7",
        "url": "^0.11.0"
    },
    "devDependencies": {
        "@types/amqplib": "^0.5.11",
        "@types/core-js": "^2.5.0",
        "@types/ibm_db": "^2.0.5",
        "@types/lodash-es": "^4.17.3",
        "@types/nano": "^7.0.0",
        "@types/node": "^11.13.8",
        "@types/pug": "^2.0.4",
        "@types/request": "^2.48.1",
        "@types/request-promise-native": "^1.0.16",
        "@types/webpack": "^4.4.29",
        "awesome-typescript-loader": "^5.2.1",
        "copy-webpack-plugin": "^5.0.3",
        "cross-env": "^5.2.0",
        "nodemon-webpack-plugin": "^4.0.8",
        "npm-run-all": "^4.1.5",
        "tslint": "^5.16.0",
        "tslint-eslint-rules": "^5.4.0",
        "tslint-sonarts": "^1.9.0",
        "typescript": "^3.4.5",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.1",
        "webpack-closure-compiler": "^2.1.6"
    }
}